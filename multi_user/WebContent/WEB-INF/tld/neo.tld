<?xml version="1.0" encoding="UTF-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
    version="2.0">
<description>JSTL 1.1 functions library</description>
  <display-name>JSTL XML</display-name>
  <tlib-version>1.1</tlib-version>
  <short-name>neo</short-name>
  <function>
   <description>String to DateTime</description>
   <name>todate</name>
   <function-class>kr.co.neodreams.multi_user.common.TldUtil</function-class>
   <function-signature>java.lang.String getDateTmFormat(java.lang.String , java.lang.String, java.lang.String)</function-signature>
  </function>
  <function>
   <description>String to HP</description>
   <name>hp</name>
   <function-class>kr.co.neodreams.multi_user.common.TldUtil</function-class>
   <function-signature>java.lang.String hpFormat(java.lang.String)</function-signature>
  </function>
  <function>
   <description>String to Bizno</description>
   <name>bizno</name>
   <function-class>kr.co.neodreams.multi_user.common.TldUtil</function-class>
   <function-signature>java.lang.String bizFormat(java.lang.String)</function-signature>
  </function>
  <function>
   <description>한전에서 사용한 커스텀 날짜 변환</description>
   <name>changday</name>
   <function-class>kr.co.neodreams.multi_user.common.TldUtil</function-class>
   <function-signature>java.lang.String changday(java.lang.String)</function-signature>
  </function>
  <function>
